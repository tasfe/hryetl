package com.banggo.scheduler.dao.dataobject;

import java.util.Date;

public class ScheManualTrigger {
	public final static String TRIGGERTYPE_TERMINATE = "1"; // 停止 
	public final static String TRIGGERTYPE_TRIGGER = "2"; // 触发 (任务执行前)
	public final static String TRIGGERTYPE_TRIGGER_FORCE = "3"; // 强制触发  (任务执行前)
	public final static String TRIGGERTYPE_TRIGGER_MANUAL = "m"; // 记录人工触发  (任务执行中)
	
	private Integer id;

   
    private Integer scheJobId;

   
    private Integer scheExecId;

   
    private String triggerType;

   
    private Date operateTime;

  
    private String operateBy;


	public Integer getId() {
		return id;
	}


	public void setId(Integer id) {
		this.id = id;
	}


	public Integer getScheJobId() {
		return scheJobId;
	}


	public void setScheJobId(Integer scheJobId) {
		this.scheJobId = scheJobId;
	}


	public Integer getScheExecId() {
		return scheExecId;
	}


	public void setScheExecId(Integer scheExecId) {
		this.scheExecId = scheExecId;
	}


	public String getTriggerType() {
		return triggerType;
	}


	public void setTriggerType(String triggerType) {
		this.triggerType = triggerType;
	}


	public Date getOperateTime() {
		return operateTime;
	}


	public void setOperateTime(Date operateTime) {
		this.operateTime = operateTime;
	}


	public String getOperateBy() {
		return operateBy;
	}


	public void setOperateBy(String operateBy) {
		this.operateBy = operateBy;
	}

   
}