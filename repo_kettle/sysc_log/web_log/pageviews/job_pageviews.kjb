      <name>SQL</name>
      <description/>
      <type>SQL</type>
      <sql>delete from bi_page_views where logtime&#x3d;&#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;insert into bi_page_views&#x28;logtime,cvid,cid,sid,userid,log_time,next_time,cts,at,new_at,ft,ad,rn,rn2,dl,dr,ref_domain,keywords,ua,sr,mobile_flag,user_ip,country,province,city,tl,utmz,bid,bid_num&#x29;&#xd;&#xa;select t.logtime,t.args.cvid as cvid,t.args.cid as cid,t.args.sid sid,t.args.&#x22;uid&#x22; as userid ,log_time,lead&#x28;log_time,1,log_time&#x29; over &#x28;partition by t.args.cvid,t.args.cid,t.args.sid order by t.log_time&#x29; as next_time,&#xd;&#xa;  &#x28;oracle_to_unix&#x28;lead&#x28;log_time,1,log_time&#x29; over &#x28;partition by t.args.cvid,t.args.cid,t.args.sid order by t.log_time&#x29;&#x29;-oracle_to_unix&#x28;log_time&#x29;&#x29; as cts,&#xd;&#xa;  t.args.at,&#xd;&#xa;   case when to_char&#x28;unix_to_oracle&#x28;t.args.ft&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x3d;to_char&#x28;unix_to_oracle&#x28;t.args.ct&#x29;,&#x27;yyyyMMdd&#x27;&#x29; then 1 else 0 end as new_at,&#xd;&#xa;  unix_to_oracle&#x28;t.args.ft&#x29; as ft,&#xd;&#xa;t.args.ad,&#xd;&#xa;row_number&#x28;&#x29; over&#x28;partition by t.args.cvid,t.args.cid,t.args.sid order by t.log_time&#x29; as rn,&#xd;&#xa;row_number&#x28;&#x29; over&#x28;partition by t.args.cvid,t.args.cid,t.args.sid order by t.log_time desc&#x29; as rn2&#xd;&#xa;,t.args.dl,t.args.dr,&#xd;&#xa;first_value&#x28;case when parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; is null or parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;hairongyi&#x25;&#x27; or parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;kjtpay&#x25;&#x27; then &#x27;-&#x27; else parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; end &#x29;&#xd;&#xa;over &#x28;partition by t.args.cid,t.args.sid order by log_time &#x29; as ref_domain, &#xd;&#xa;case&#xd;&#xa;  when parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;m.baidu&#x25;&#x27; then url_decode&#x28;parseurl_para&#x28;t.args.dr,&#x27;query&#x27;,&#x27;word&#x27;&#x29;&#x29;&#xd;&#xa;  when parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;baidu&#x25;&#x27; then url_decode&#x28;parseurl_para&#x28;t.args.dr,&#x27;query&#x27;,&#x27;wd&#x27;&#x29;&#x29;&#xd;&#xa;  when parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;sogou&#x25;&#x27; then url_decode&#x28;parseurl_para&#x28;t.args.dr,&#x27;query&#x27;,&#x27;query&#x27;&#x29;&#x29;&#xd;&#xa;  when parseurl&#x28;t.args.dr,&#x27;host&#x27;&#x29; like &#x27;&#x25;so.com&#x25;&#x27; then url_decode&#x28;parseurl_para&#x28;t.args.dr,&#x27;query&#x27;,&#x27;q&#x27;&#x29;&#x29;&#xd;&#xa;  else &#x27;-&#x27; end as keywords,ua,t.args.sr,&#xd;&#xa;    case when REGEXP_LIKE&#x28;t.ua, &#x27;android&#x7c;mobile&#x7c;Adr&#x27;, &#x27;i&#x27;&#x29; then 1 else 0 end mobile_flag,&#xd;&#xa;t.user_ip,t.country2 as country,t.province2 as province,t.city2 as city,&#xd;&#xa;t.args.tl as tl,url_decode&#x28;url_decode&#x28;url_decode&#x28;t.args.utmz&#x29;&#x29;&#x29; as utmz,&#x28;t.args.bid &#x7c;&#x7c; parseurl_para&#x28;t.args.dl,&#x27;query&#x27;,&#x27;bId&#x27;&#x29;&#x29; as bid,&#xd;&#xa;case when &#x28;t.args.bid &#x7c;&#x7c; parseurl_para&#x28;t.args.dl,&#x27;query&#x27;,&#x27;bId&#x27;&#x29; &#x29; is not null then count&#x28;&#x28;t.args.bid &#x7c;&#x7c; parseurl_para&#x28;t.args.dl,&#x27;query&#x27;,&#x27;bId&#x27;&#x29; &#x29;&#x29; over &#x28;partition by logtime,t.args.cid,t.args.sid,&#x28;t.args.bid &#x7c;&#x7c; parseurl_para&#x28;t.args.dl,&#x27;query&#x27;,&#x27;bId&#x27;&#x29;&#x29;&#x29; else 0 end as bid_num&#xd;&#xa;from bi_web_log t where t.args.ad&#x3d;108 and length&#x28;t.args.cvid&#x29;&#x3c;50 and length&#x28;t.args.sid&#x29;&#x3c;50 and logtime&#x3d;&#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;-- web&#x767b;&#x5f55;&#x7528;&#x6237;&#xd;&#xa;delete from bi_login_web where logtime &#x3d; &#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;insert into bi_login_web &#x28;logtime, cid, sid, userid, log_time, ft, mobile_flag, ua, user_ip,province,city&#x29;&#xd;&#xa;select logtime,cid,sid,userid,log_time,ft,mobile_flag,ua,user_ip,province,city from&#xd;&#xa;&#x28;&#xd;&#xa;select logtime,cid,sid,userid,log_time,ft,mobile_flag,ua,user_ip,province,city,&#xd;&#xa;row_number&#x28;&#x29; over&#x28;partition by logtime,cid,sid,userid order by log_time&#x29; as rn&#xd;&#xa; from bi_page_views where logtime &#x3d; &#x24;&#x7b;start_date&#x7d; and userid is not null &#xd;&#xa;&#x29; t where rn&#x3d;1&#x3b;&#xd;&#xa;&#xd;&#xa;-- &#xd;&#xa;delete from bi_page_cliks where logtime&#x3d;&#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;insert into bi_page_cliks&#x28;logtime,dl,av,cls_uv,cls_new_uv,cliks&#x29;&#xd;&#xa;select logtime,case when INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;&#x3d;0 then t.args.dl else SUBSTR&#x28;t.args.dl,0,INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;-1&#x29; end as dl,&#xd;&#xa; t.args.av as av,&#xd;&#xa; count&#x28;distinct t.args.cid&#x29; as cls_uv,count&#x28;distinct case when t.args.at&#x3d;1 then t.args.cid else null end&#x29; as cls_new_uv,count&#x28;1&#x29; as cliks&#xd;&#xa;from bi_web_log t where logtime&#x3d;&#x24;&#x7b;start_date&#x7d; and t.args.ad not in&#x28;108,200&#x29; and t.args.mt&#x3d;1 and t.args.dl is not null and t.args.av is not null&#xd;&#xa;group by logtime,case when INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;&#x3d;0 then t.args.dl else SUBSTR&#x28;t.args.dl,0,INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;-1&#x29; end,t.args.av&#xd;&#xa;having  count&#x28;1&#x29;&#x3e;2&#xd;&#xa;union&#xd;&#xa;select logtime,case when INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;&#x3d;0 then t.args.dl else SUBSTR&#x28;t.args.dl,0,INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;-1&#x29; end as dl,&#x27;&#x8f93;&#x5165;&#x6846;&#x27; as av,&#xd;&#xa; count&#x28;distinct t.args.cid&#x29; as cls_uv,count&#x28;distinct case when t.args.at&#x3d;1 then t.args.cid else null end&#x29; as cls_new_uv,count&#x28;1&#x29; as cliks &#xd;&#xa;from bi_web_log t where logtime&#x3d;&#x24;&#x7b;start_date&#x7d; and t.args.ad &#x3d; 105 and t.args.mt&#x3d;1 and t.args.dl is not null &#xd;&#xa;and &#x28;t.args.dl like &#x27;https&#x3a;&#x2f;&#x2f;www.hairongyi.com&#x2f;bid&#x2f;&#x25;&#x27; or t.args.dl&#x3d;&#x27;https&#x3a;&#x2f;&#x2f;www.hairongyi.com&#x2f;user&#x2f;fund&#x2f;charge.html&#x27;&#x29;&#xd;&#xa;and t.args.av is null and t.args.dstl is null&#xd;&#xa;group by logtime,case when INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;&#x3d;0 then t.args.dl else SUBSTR&#x28;t.args.dl,0,INSTR&#x28;t.args.dl,&#x27;&#x3f;&#x27;,1,1&#x29;-1&#x29; end,t.args.av&#xd;&#xa;having  count&#x28;1&#x29;&#x3e;2&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- top&#x5730;&#x533a;&#x5206;&#x6790;&#xd;&#xa;delete from bi_top_addr_pvuv_dt where dt&#x3d;&#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;insert into bi_top_addr_pvuv_dt&#x28;dt, province, city, uv, pv, ip, sv, login_users, new_uv, avg_page_times, avg_user_times, return_rate, avg_pv, avg_sv, new_uv_precent, pc_uv,rn&#x29; &#xd;&#xa;select &#x2a; from&#xd;&#xa;&#x28;&#xd;&#xa;select logtime as dt,province,&#xd;&#xa;NVL&#x28;city,&#x27;&#x5176;&#x4ed6;&#x27;&#x29; as city,&#xd;&#xa;count&#x28;distinct cid&#x29; as uv,&#xd;&#xa;count&#x28;1&#x29; as pv,&#xd;&#xa;count&#x28;distinct user_ip&#x29; as ip,&#xd;&#xa;count&#x28;distinct sid&#x29; as sv,&#xd;&#xa;count&#x28;distinct userid&#x29; as login_users,&#xd;&#xa;count&#x28;distinct case when new_at&#x3d;1 then cid else null end&#x29; as new_uv,&#xd;&#xa;round&#x28;avg&#x28;cts&#x29;,2&#x29; as avg_page_times,&#xd;&#xa;case when count&#x28;distinct sid&#x29; &#x3d;0 then 0 else round&#x28;sum&#x28;cts&#x29;&#x2f;count&#x28;distinct sid&#x29;,2&#x29; end as avg_user_times,&#xd;&#xa;case when count&#x28;distinct sid&#x29;&#x3d;0 then 0 else round&#x28;sum&#x28;case when rn&#x3d;1 and rn&#x3d;rn2 then 1 else 0 end&#x29;&#x2f;count&#x28;distinct sid&#x29;&#x2a;100,2&#x29; end as return_rate,&#xd;&#xa;case when count&#x28;distinct cid&#x29;&#x3d;0 then 0 else round&#x28;count&#x28;1&#x29;&#x2f;count&#x28;distinct cid&#x29;,2&#x29; end as avg_pv,&#xd;&#xa;case when count&#x28;distinct cid&#x29;&#x3d;0 then 0 else round&#x28;count&#x28;distinct sid&#x29;&#x2f;count&#x28;distinct cid&#x29;,2&#x29; end as avg_sv,&#xd;&#xa;case when count&#x28;distinct cid&#x29;&#x3d;0 then 0 else round&#x28;count&#x28;distinct case when new_at&#x3d;1 then cid else null end&#x29;&#x2f;count&#x28;distinct cid&#x29;&#x2a;100,2&#x29; end as new_uv_precent,&#xd;&#xa;count&#x28;distinct case when mobile_flag&#x3d;0 then cid else null end&#x29; as pc_uv,&#xd;&#xa;row_number&#x28;&#x29; over&#x28;partition by logtime order by count&#x28;distinct cid&#x29; desc&#x29; rn&#xd;&#xa;from bi_page_views where logtime&#x3d;&#x24;&#x7b;start_date&#x7d;&#xd;&#xa;group by logtime,province,NVL&#x28;city,&#x27;&#x5176;&#x4ed6;&#x27;&#x29;&#xd;&#xa;order by dt,rn&#xd;&#xa;&#x29; t where rn&#x3c;&#x3d;50&#x3b;&#xd;&#xa;&#xd;&#xa;-- top&#x57df;&#x540d;&#x5206;&#x6790;&#xd;&#xa;delete from bi_top_domain_pvuv_dt where dt&#x3d;&#x24;&#x7b;start_date&#x7d;&#x3b;&#xd;&#xa;insert into bi_top_domain_pvuv_dt&#x28;dt,ref_domain, uv, pv, ip, sv, login_users, new_uv, avg_page_times, avg_user_times, return_rate, avg_pv, avg_sv, new_uv_precent, pc_uv,rn&#x29; &#xd;&#xa;select &#x2a; from&#xd;&#xa;&#x28;&#xd;&#xa;select logtime as dt,&#xd;&#xa;case when ref_domain is null or ref_domain &#x3d;&#x27;-&#x27; then &#x27;&#x81ea;&#x4e3b;&#x8bbf;&#x95ee;&#x27; else ref_domain end as ref_domain,&#xd;&#xa;count&#x28;distinct cid&#x29; as uv,&#xd;&#xa;count&#x28;1&#x29; as pv,&#xd;&#xa;count&#x28;distinct user_ip&#x29; as ip,&#xd;&#xa;count&#x28;distinct sid&#x29; as sv,&#xd;&#xa;count&#x28;distinct userid&#x29; as login_users,&#xd;&#xa;count&#x28;distinct case when new_at&#x3d;1 then cid else null end&#x29; as new_uv,&#xd;&#xa;round&#x28;avg&#x28;cts&#x29;,2&#x29; as avg_page_times,&#xd;&#xa;case when count&#x28;distinct sid&#x29; &#x3d;0 then 0 else round&#x28;sum&#x28;cts&#x