      <sql>update bi_browser_ua t set t.mobile_flag&#x3d;1 WHERE REGEXP_LIKE&#x28;t.ua, &#x27;android&#x7c;mobile&#x7c;Adr&#x27;, &#x27;i&#x27;&#x29; and t.mobile_flag &#x3c;&#x3e; 1 &#x3b; &#xd;&#xa;update bi_browser_ua t set t.mobile_flag&#x3d;0 WHERE t.mobile_flag is null&#x3b;&#xd;&#xa;update bi_browser_ua t set agent_type &#x3d;&#x27;Browser&#x27; where agent_type &#x3d;&#x27;unknown&#x27;&#x3b;&#xd;&#xa;update bi_browser_ua t set agent_type &#x3d;&#x27;Android Webkit Browser&#x27; where agent_name &#x3d;&#x27;unknown&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;Webkit&#x27;, &#x27;i&#x27;&#x29;&#x3b;&#xd;&#xa;update bi_browser_ua t set os_type &#x3d;&#x27;Android&#x27; , os_name&#x3d;&#x27;Android&#x27; where os_type &#x3d;&#x27;Linux&#x27; and os_name&#x3d;&#x27;Linux&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;android&#x7c;adr&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1&#x3b;&#xd;&#xa;--&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;UC&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;ucweb&#x7c;ucbrowser&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x641c;&#x72d7;&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;sogou&#x7c;MetaSr&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Opera&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;Opera&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;QQ&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;QQBrowser&#x7c;TencentTraveler&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;360&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;360SE&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x4e16;&#x754c;&#x4e4b;&#x7a97;&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;world&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x9068;&#x6e38;&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;Maxthon&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x767e;&#x5ea6;&#x6d4f;&#x89c8;&#x5668;&#x27; where os_name&#x3d;&#x27;Android&#x27; and os_type &#x3d;&#x27;Android&#x27; and REGEXP_LIKE&#x28;t.ua, &#x27;baidu&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;1 and browers_name is null &#x3b;&#xd;&#xa;--&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;UC&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;ucweb&#x7c;ucbrowser&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x641c;&#x72d7;&#x6d4f;&#x89c8;&#x5668;&#x27; where  REGEXP_LIKE&#x28;t.ua, &#x27;sogou&#x7c;MetaSr&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Opera&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;Opera&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;QQ&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;QQBrowser&#x7c;TencentTraveler&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;360&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;360SE&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x4e16;&#x754c;&#x4e4b;&#x7a97;&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;world&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x9068;&#x6e38;&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;Maxthon&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x767e;&#x5ea6;&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;baidu&#x27;, &#x27;i&#x27;&#x29; and mobile_flag&#x3d;0 and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Chrome&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;Chrome&#x27;, &#x27;i&#x27;&#x29; and browers_name is null &#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Safari&#x6d4f;&#x89c8;&#x5668;&#x27; where REGEXP_LIKE&#x28;t.ua, &#x27;Safari&#x27;, &#x27;i&#x27;&#x29; and browers_name is null&#x3b;&#xd;&#xa;-- update bi_browser_ua t set browers_name &#x3d;&#x27;Safari&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;Safari&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;IE&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;Internet Explorer&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;IE Mobile&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;IE Mobile&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Firefox&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;Firefox&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;Netscape&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;Netscape&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;UC&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;UC Browser&#x27; and browers_name is null&#x3b;&#xd;&#xa;update bi_browser_ua t set browers_name &#x3d;&#x27;&#x7eff;&#x8272;&#x6d4f;&#x89c8;&#x5668;&#x27; where t.agent_name&#x3d;&#x27;GreenBrowser&#x27; and browers_name is null&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
