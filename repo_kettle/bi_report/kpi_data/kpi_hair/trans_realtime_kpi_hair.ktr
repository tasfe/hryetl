    <sql>&#xd;&#xa;-- &#x7d2f;&#x8ba1;&#x57fa;&#x91d1;&#x4ea4;&#x6613;&#x91d1;&#x989d;&#xd;&#xa;select &#x27;1&#x27; as flag,&#x27;fund&#x27; as prod,&#x27;amt&#x27;as type,SUM&#x28;F.TRANS_AMT&#x29;&#x2f;100 as stats_value from FUNDUSER.T_FUND_SHARE_ORDER&#x40;FDB F WHERE F.STATUS&#x3d;&#x27;05&#x27; and f.TRANS_TYPE &#x3d;&#x27;I&#x27;&#xd;&#xa;union all&#xd;&#xa;-- &#x7d2f;&#x8ba1;&#x6d77;&#x5b9d;&#x4ea4;&#x6613;&#x91d1;&#x989d;     &#x65e0;&#x6570;&#x636e;&#x66f4;&#x65b0;&#xd;&#xa;-- select &#x27;1&#x27; as flag,&#x27;hb&#x27; as prod,&#x27;amt&#x27;as type,to_number&#x28;max&#x28;TOTAL_DEAL&#x29;&#x29; as stats_value  from s_s71_HB_INFOMATION&#xd;&#xa;-- union all&#xd;&#xa;-- &#x7d2f;&#x8ba1;&#x7ebf;&#x4e0b;&#x4ea4;&#x6613;&#x91d1;&#x989d;&#xd;&#xa;select &#x27;1&#x27; as flag,&#x27;offline&#x27; as prod,&#x27;amt&#x27; as type,SUM&#x28;trade_amount&#x29; stats_value from s_s70_hry_offline_trade_stats&#xd;&#xa;union all&#xd;&#xa;--&#x7d2f;&#x8ba1;&#x6ce8;&#x518c;&#x7528;&#x6237;&#x6570;&#xd;&#xa;select &#x27;1&#x27; as flag,&#x27;hry&#x27; as prod,&#x27;reg_users&#x27; as type, count&#x28;1&#x29; stats_value  FROM MEMBER.TM_MEMBER&#x40;KJTDB B -- where b.member_type&#x3d;1&#xd;&#xa;union all&#xd;&#xa;-- &#x5f53;&#x65e5;&#x57fa;&#x91d1;&#x4ea4;&#x6613;&#x91d1;&#x989d;&#xd;&#xa;select &#x27;0&#x27; as flag,&#x27;fund&#x27; as prod,&#x27;amt&#x27;as type, SUM&#x28;F.TRANS_AMT&#x29;&#x2f;100 as stats_value from FUNDUSER.T_FUND_SHARE_ORDER&#x40;FDB F WHERE F.STATUS&#x3d;&#x27;05&#x27; and f.TRANS_TYPE &#x3d;&#x27;I&#x27; and create_time&#x3e;&#x3d;to_timestamp&#x28;to_char&#x28;sysdate,&#x27;yyyymmdd&#x27;&#x29;,&#x27;yyyymmdd hh24&#x3a;mi&#x3a;ss&#x27;&#x29;&#xd;&#xa;union all&#xd;&#xa;-- &#x5f53;&#x65e5;&#x6ce8;&#x518c;&#x7528;&#x6237;&#x6570;&#xd;&#xa;select &#x27;0&#x27; as flag,&#x27;hry&#x27; as prod,&#x27;reg_users&#x27;as type,count&#x28;1&#x29; as stats_value FROM MEMBER.TM_MEMBER&#x40;KJTDB B where -- b.member_type&#x3d;1 and &#xd;&#xa;create_time&#x3e;&#x3d;to_timestamp&#x28;to_char&#x28;sysdate,&#x27;yyyymmdd&#x27;&#x29;,&#x27;yyyymmdd hh24&#x3a;mi&#x3a;ss&#x27;&#x29;</sql>
