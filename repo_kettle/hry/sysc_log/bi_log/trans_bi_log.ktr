        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.Arrays;
import com.hry.IpLoc;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	if (first){
	  first = false;
	}


	Object[] r = getRow();

	if (r == null) {
	  setOutputDone();
	  return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	
	String ip = get(Fields.In, "ip_add").getString(r);
	// IP.load("17monipdb.dat");
	String country = null;
	String province = null;
	String city = null;

	try {
		// String[] add=IP.find(ip);
        String[] add = IpLoc.getInstance().find(ip);
		country=add[0];
		province=add[1];
		city=add[2];

		} catch (Exception e) {
			country=null;
		    province=null;
		    city=null;
		}


 get(Fields.Out, "country").setValue(r, country);
 get(Fields.Out, "province").setValue(r, province);
 get(Fields.Out, "city").setValue(r, city);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
