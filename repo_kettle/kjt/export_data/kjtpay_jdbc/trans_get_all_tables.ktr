        <class_source><![CDATA[static  String[] tList = null;
static  String[] tnList =null;
static String con=null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    

	if (first){
	  first = false;
	  String tablename = getVariable("id", "");
	  tList = (null == tablename || "".equals(tablename)) ? null : tablename.split(",");

      con = getVariable("cond", "");

	  String tablename_no = getVariable("id_no", "");
	  tnList = (null == tablename_no || "".equals(tablename_no)) ? null : tablename_no.split(","); 		

	}
   

	Object[] r = getRow();

	if (r == null) {
	  setOutputDone();
	  return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	
    String tn= get(Fields.In, "ID").getString(r);

if(con != null && con.length()>0){

String ids=tList[0];
if(">".equalsIgnoreCase(con)){
if(Integer.valueOf(tn)>Integer.valueOf(ids)){ putRow(data.outputRowMeta, r); }

}else if(">=".equalsIgnoreCase(con)){
if(Integer.valueOf(tn)>=Integer.valueOf(ids)){ putRow(data.outputRowMeta, r); }

}else if("=".equalsIgnoreCase(con)){
if(Integer.valueOf(tn)==Integer.valueOf(ids)){ putRow(data.outputRowMeta, r); }

}else if("<".equalsIgnoreCase(con)){
if(Integer.valueOf(tn)<Integer.valueOf(ids)){ putRow(data.outputRowMeta, r); }

}else if("<=".equalsIgnoreCase(con)){
if(Integer.valueOf(tn)<=Integer.valueOf(ids)){ putRow(data.outputRowMeta, r); }

}else {
logError("有问题");
}	

}else {
	
	if(null != tList && tList.length>0){
		for (int i = 0; i < tList.length; i++) {
			String tnp = tList[i];
			if(null != tn && tnp.equalsIgnoreCase(tn)){
              
				    // 过滤的表
					if(null != tnList && tnList.length>0){
						boolean flag = true;
						for (int j = 0; j < tnList.length; j++) {
							String tnp2 = tnList[j];
							if(null != tn && tnp2.equalsIgnoreCase(tn)){ flag=false; break;}
						}
							if(flag){ putRow(data.outputRowMeta, r); }
					}else{
							putRow(data.outputRowMeta, r);
					}
				    // 过滤的表


			  break;
			}
			
		}
	}else{ 

// 过滤的表
		if(null != tnList && tnList.length>0){
            boolean flag = true;
			for (int i = 0; i < tnList.length; i++) {
				String tnp2 = tnList[i];
				if(null != tn && tnp2.equalsIgnoreCase(tn)){ flag=false; break;}
		
			}
			if(flag){ putRow(data.outputRowMeta, r); }

		}else{
			putRow(data.outputRowMeta, r);
		}
// 过滤的表

	}
}
	return true;
}
